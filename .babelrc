{
  "env": {
    "development": {
      "plugins": [
        "stack-trace-sourcemap"
      ]
    },
    "test": {
      "plugins": [
        "stack-trace-sourcemap"
      ]
    }
  },
  "presets": [["es2015", {"loose": true}], "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "transform-object-assign",
    "transform-react-display-name",
    "transform-runtime"
  ]
}
